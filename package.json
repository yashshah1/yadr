    "precommit": "npm run format && npm run lint && npm run test -- --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format && npm run lint && npm run test -- --coverage"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Yash Shah <yashah1234@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yashshah1/yadr.git"
  },
  "bugs": {
    "url": "https://github.com/yashshah1/yadr/issues",
    "email": "yashah1234@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "<26.0.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
