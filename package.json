{
  "name": "yadr",
  "version": "1.0.0",
  "description": "Yet another collection of datastructures, but in TS",
  "keywords": [
    "datastructures",
    "typescript"
  ],
  "homepage": "https://github.com/yashshah1/yadr",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
